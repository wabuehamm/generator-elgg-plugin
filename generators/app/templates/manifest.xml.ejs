<?xml version="1.0" encoding="UTF-8"?>
<plugin_manifest xmlns="http://www.elgg.org/plugin_manifest/1.8">
    <name><%= name %></name>
    <author><%= author %></author>
    <version type="dist">0.0.0</version>
    <id><%= id %></id>
    <description>
        <%= description %>
    </description>
    <website><%= website %></website>
    <license><%= license %></license>
    <category><%= category %></category>

    <requires>
        <type>elgg_release</type>
        <version><%= elggRelease %></version>
    </requires>
    
    <!-- Please check out http://learn.elgg.org/en/stable/guides/plugins/dependencies.html if you need more dependency declarations -->

</plugin_manifest>
